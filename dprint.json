{
  "incremental": true,
  "exec": {
    "associations": "**/*.{lua,sh,env}",
    "timeout": 30,
    "stylua": "stylua -f stylua.toml -",
    "stylua.associations": "**/*.lua",
    "shfmt": "shfmt -",
    "shfmt.associations": "**/*.{sh,env,zsh}"
  },
  "json": {},
  "markdown": {},
  "toml": {},
  "includes": [
    "**/*.{json,md,toml,lua,css,scm,sh,env}"
  ],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/json-0.15.6.wasm",
    "https://plugins.dprint.dev/markdown-0.14.1.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/exec-0.3.1.json@9351b67ec7a6b58a69201c2834cba38cb3d191080aefc6422fb1320f03c8fc4d"
  ]
}
